{
  "expo": {
    "name": "Bus Radar Tampere",
    "slug": "BusRadar",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "fi.tuni.tamk.tiko.busradarnative",
      "config": {
        "googleMaps": {
          "apiKey": "api key goes here"
        }
      }
    }
  }
}
